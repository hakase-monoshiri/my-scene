<%= form_for concert do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :description %>
    <%= f.text_area :description %><br>

    <%= f.label :start_time%>
    <%= f.datetime_field :start_time %><br>

    <%= f.label :end_time%>
    <%= f.datetime_field :end_time %><br>

    <%= artist_id_field(concert)%>
    <%= venue_id_field(concert) %>
  <%= f.submit %>
<% end %> 