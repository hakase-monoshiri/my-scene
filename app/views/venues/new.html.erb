<h1> New Venue </h1>

<% if @venue.errors.any? %>
  <div id="error_explanation">
    <h2>There were some errors:</h2>
    <ul>
      <% @venue.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </ul>
  </div>
<% end %> 

<%= form_tag venues_path do %>
<%= render partial: "form", locals: {venue: @venue, address: @address}%>
<% end %>

<br>
<%= link_to "Back to Venues", venues_path %>
